<launch>
  <!-- <include
    file="$(find gazebo_ros)/launch/empty_world.launch" /> -->

    <arg name="world_name" value="$(find agv_urdf_test)/world/world_room.world"/>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
      <arg name="world_name" value="$(arg world_name)" />
      <!-- <arg name="use_sim_time" value="false"/> -->
    </include>


  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find agv_real)/urdf/agv_real.urdf -urdf -model agv_real"
    output="screen" />

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" >
      <remap from="/joint_states" to="/agv_sim/joint_states"/>
    </node>

    <!-- 激光雷达融合节点 -->
  <node pkg="ira_laser_tools" name="laserscan_multi_merger" type="laserscan_multi_merger" output="screen">
    <param name="destination_frame" value="base_footprint"/>
    <param name="cloud_destination_topic" value="/merged_cloud"/>
    <param name="scan_destination_topic" value="/scan"/>
    <param name="laserscan_topics" value ="/scan_1 /scan_2" /> <!-- LIST OF THE LASER SCAN TOPICS TO SUBSCRIBE -->
    <param name="angle_min" value="-3.1415926"/>
    <param name="angle_max" value="3.1415926"/>

  </node>



</launch>