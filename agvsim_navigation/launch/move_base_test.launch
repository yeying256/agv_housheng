    <rosparam file="$(find agvsim_navigation)/param/move_base_params.yaml" command="load" />
      <rosparam file="$(find agvsim_navigation)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
      <rosparam file="$(find agvsim_navigation)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
      <rosparam file="$(find agvsim_navigation)/param/local_costmap_params.yaml" command="load" />
